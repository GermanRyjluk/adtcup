{
  "expo": {
    "name": "ADTCUP",
    "slug": "ADTCUP",
    "version": "1.0.5",
    "targetSDKversion": "33",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B4D98"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow ADT CUP to use your location to check if you are on the right spot to start the game!",
        "NSCameraUsageDescription": "Allow ADT CUP to use your camera to be able to read the QR codes that give you new quiz to play with!"
      },
      "bundleIdentifier": "com.ryyuuk.ADTCUP",
      "buildNumber": "1.0.5"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FEAE00"
      },
      "versionCode": 6,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.ryyuuk.ADTCUP"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow ADT CUP to use your location to check if you are on the right spot to start the game!"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1ba79ce8-d2ce-40fc-97c4-b2eeca5f889a"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1ba79ce8-d2ce-40fc-97c4-b2eeca5f889a"
    }
  }
}